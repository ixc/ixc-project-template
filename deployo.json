{
	"project_slug": "{{ project_name }}",
	"environments": {
		"prd": {
			"default_server": "{{ project_name }}.ixchosted.com",
			"ssh_port": "22022",
			"default_user": "ixc",
			"project_directory": "/ixc/{{ project_name }}-prd/app",
			"source_directory": "/ixc/{{ project_name }}-prd/app/{% verbatim %}{{ repos_name }}{% endverbatim %}",
			"virtualenv_directory": "/ixc/{{ project_name }}-prd/app/{% verbatim %}{{ repos_name }}{% endverbatim %}/venv",
			"git_branch": "master",
			"tags": ["system-supervisor"]
		},
		"stg": {
			"default_server": "{{ project_name }}.stg.ixcsandbox.com",
			"ssh_port": "22022",
			"default_user": "ixc",
			"project_directory": "/ixc/{{ project_name }}-stg/app",
			"source_directory": "/ixc/{{ project_name }}-stg/app/{% verbatim %}{{ repos_name }}{% endverbatim %}",
			"virtualenv_directory": "/ixc/{{ project_name }}-stg/app/{% verbatim %}{{ repos_name }}{% endverbatim %}/venv",
			"git_branch": "staging",
			"tags": ["system-supervisor"]
		}
	}
}
